{
  "name": "cnode-react",
  "version": "1.0.0",
  "description": "cnode-react",
  "main": "index.js",
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunhaoxiang/cnode-react.git"
  },
  "author": "sunhaoxiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sunhaoxiang/cnode-react/issues"
  },
  "homepage": "https://github.com/sunhaoxiang/cnode-react#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
